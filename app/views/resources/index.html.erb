<h1> Cstor </h1>
<%= link_to 'Create New Resource', new_resource_path %>
<ul class="all-resources">
<% @resources.each do |resource| %>
<p>
  <h2>
    <a href="/resources/<%= resource.id%>"><%= resource.title %></a>
  </h2>
</p>
<p> <%= resource.abstract %> </p>
<p> <em><%= resource.teacher.username %></em> </p>
<% end %>
</ul>
